{
  "manifest_version": 2,
  "name": "Tree Style Tab in Separate Window",
  "version": "5.2",
  "applications": {
    "gecko": {
      "id": "{ad983116-67d4-49d3-85ff-e7f46ab73dac}",
      "strict_min_version": "60.0"
    }
  },
  "default_locale": "en",
  "permissions": [
    "storage",
    "menus",
    "notifications"
  ],
  "optional_permissions": [
    "tabs",
    "sessions"
  ],
  "options_ui": {
    "page": "options/options.html"
  },
  "background": {
    "page": "background/background.html"
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/popup-16-light.png",
      "32": "icons/popup-32-light.png"
    },
    "theme_icons": [
      {
        "light": "icons/popup-16-dark.png",
        "dark": "icons/popup-16-light.png",
        "size": 16
      },
      {
        "light": "icons/popup-32-dark.png",
        "dark": "icons/popup-32-light.png",
        "size": 32
      }
    ]
  },
  "commands": {
    "open-tst-sidebar-in-tab": {
      "description": "__MSG_contextMenu_openSidebarInTab__"
    },
    "open-tst-sidebar-in-window": {
      "description": "__MSG_contextMenu_openSidebarInWindow__"
    },
    "open-tst-sidebar-in-docked-window": {
      "description": "__MSG_contextMenu_openSidebarInDockedWindow__"
    }
  }
}